import java.util.*;

public class Main
{
    public static void main(String[] args) {
        double altura;
        double resultado;
        
        System.out.println("Digite uma altura");
        Scanner scan = new Scanner(System.in);
        altura = scan.nextDouble();
        
        resultado = (72.7 + altura) - 58;
        System.out.println("O resultado Ã© " + resultado);
        
    }
}
